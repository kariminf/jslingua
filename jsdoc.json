{
  "source": {
        "exclude": ["docs", "out", "test", ".git", "dist", "node_modules"],
        "includePattern": "src/[^/]+\\.m?js$"
    },
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "recurseDepth": 1
  },
  "templates": {
    "default": {
      "staticFiles": {
        "include": [
            "./assets/design/logo.png"
        ]
      }
    },
    "logo": {
      "url": "logo.png",
      "width": "50px",
      "height": "50px",
      "link": "https://kariminf.github.io/jslingua.web/"
    },
    "name": "JsLingua doc"
  }
}
